<div class="contentprof">
  <%= image_tag current_user.icon.url, width: 150, class: "prof" %>
  <%= current_user.name %>
</div>

<div class="contents_image2">
  <% @pictures.reverse_each do |picture| %>
    <div class="contents_image3">
      <%= link_to picture_path(picture.id) do %>
        <%= image_tag picture.image.url, size: 200, class: "img-responsive" %>
      <% end %>
    </div>
  <% end %>
</div>
